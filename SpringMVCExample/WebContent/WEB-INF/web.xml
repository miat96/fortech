<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>SpringMVCExample</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>
           org.springframework.web.servlet.DispatcherServlet
        </servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>result</servlet-name>
    <servlet-class>
           org.springframework.web.servlet.DispatcherServlet
        </servlet-class>
    <load-on-startup>2</load-on-startup>
  </servlet>
  
  <servlet>
    <servlet-name>student</servlet-name>
    <servlet-class>
           org.springframework.web.servlet.DispatcherServlet
        </servlet-class>
    <load-on-startup>3</load-on-startup>
  </servlet>
  
  <servlet>
    <servlet-name>facultate</servlet-name>
    <servlet-class>
           org.springframework.web.servlet.DispatcherServlet
        </servlet-class>
    <load-on-startup>4</load-on-startup>
  </servlet>

	<servlet-mapping>
	    <servlet-name>login</servlet-name>
	    <url-pattern>/login</url-pattern>
	 </servlet-mapping>
	 
	 <servlet-mapping>
	    <servlet-name>result</servlet-name>
	    <url-pattern>/goStudent</url-pattern>
	 </servlet-mapping>
	 
	 <servlet-mapping>
	    <servlet-name>result</servlet-name>
	    <url-pattern>/goFacultate</url-pattern>
	 </servlet-mapping>
	 
	 <servlet-mapping>
	    <servlet-name>result</servlet-name>
	    <url-pattern>/loadStudentFromCamera</url-pattern>
	 </servlet-mapping>
	 
	 <servlet-mapping>
	    <servlet-name>login</servlet-name>
	    <url-pattern>/loadCamere</url-pattern>
	 </servlet-mapping>
	 
	 <servlet-mapping>
	    <servlet-name>result</servlet-name>
	    <url-pattern>/loadCam</url-pattern>
	 </servlet-mapping>
	 
	 
	 <servlet-mapping>
	    <servlet-name>login</servlet-name>
	    <url-pattern>/ceva</url-pattern>
	 </servlet-mapping>
	 
	 <servlet-mapping>
	    <servlet-name>student</servlet-name>
	    <url-pattern>/goResult</url-pattern>
	 </servlet-mapping>
	 
	 <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/loadUser</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/loadUniversitati</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>result</servlet-name>
        <url-pattern>/loadUniv</url-pattern>
    </servlet-mapping>
    
	 <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/findUser</url-pattern>
    </servlet-mapping>
    
      <servlet-mapping>
        <servlet-name>facultate</servlet-name>
        <url-pattern>/loadStudenti</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>result</servlet-name>
        <url-pattern>/formularAddStudent</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>result</servlet-name>
        <url-pattern>/createStudent</url-pattern>
    </servlet-mapping>
	 
	 <servlet-mapping>
        <servlet-name>student</servlet-name>
        <url-pattern>/addStudent</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>student</servlet-name>
        <url-pattern>/cautareStudentiByNume</url-pattern>
    </servlet-mapping>
    
     <servlet-mapping>
        <servlet-name>result</servlet-name>
        <url-pattern>/addCamera</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>result</servlet-name>
        <url-pattern>/stergereCamera</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>result</servlet-name>
        <url-pattern>/stergereStudent</url-pattern>
    </servlet-mapping>
	 
	 
	  <servlet-mapping>
	    <servlet-name>login</servlet-name>
	    <url-pattern>/index.html</url-pattern>
	  </servlet-mapping>
	  
	  <servlet>
	    <servlet-name>index</servlet-name>
	    <servlet-class>web.IndexServlet</servlet-class>
	  </servlet>
	 
</web-app>